function teman(nama , usia ){
    var siswa = {}
        siswa.nama = nama,
        siswa.usia = usia
    return siswa
}
console.log(teman('salman', 18));

function siswa(warna , tinggi , berat){
    var orang = {}
        orang.warna = warna,
        orang.tinggi = tinggi,
        orang.berat = berat
    return orang
}
console.log(siswa('cokelat', 183, 43));
